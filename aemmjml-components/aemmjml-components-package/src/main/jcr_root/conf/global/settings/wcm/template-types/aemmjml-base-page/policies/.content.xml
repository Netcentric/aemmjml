<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="wcm/core/components/policies/mappings">
        <root
            cq:policy="netcentric/aemmjml/foundation/components/body/default_policy"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/core/components/policies/mapping">
            <netcentric jcr:primaryType="nt:unstructured">
                <aemmjml jcr:primaryType="nt:unstructured">
                    <core jcr:primaryType="nt:unstructured">
                        <components jcr:primaryType="nt:unstructured">
                            <column
                                cq:policy="netcentric/aemmjml/core/components/column/default_policy"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/core/components/policies/mapping"/>
                            <group
                                cq:policy="netcentric/aemmjml/core/components/group/default_policy"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/core/components/policies/mapping"/>
                            <hero
                                cq:policy="netcentric/aemmjml/core/components/hero/default_policy"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/core/components/policies/mapping"/>
                            <section
                                cq:policy="netcentric/aemmjml/core/components/section/default_policy"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/core/components/policies/mapping"/>
                            <wrapper
                                cq:policy="netcentric/aemmjml/core/components/section/default_policy"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="wcm/core/components/wrapper/mapping"/>
                        </components>
                    </core>
                </aemmjml>
            </netcentric>

            <section
                cq:policy="netcentric/aemmjml/core/components/section/default_policy"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/core/components/policies/mapping">
                <netcentric jcr:primaryType="nt:unstructured">
                    <aemmjml jcr:primaryType="nt:unstructured">
                        <core jcr:primaryType="nt:unstructured">
                            <components jcr:primaryType="nt:unstructured">
                                <group
                                    cq:policy="netcentric/aemmjml/core/components/group/default_policy"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="wcm/core/components/policies/mapping"/>
                                <column
                                    cq:policy="netcentric/aemmjml/core/components/column/default_policy"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="wcm/core/components/policies/mapping"/>
                            </components>
                        </core>
                    </aemmjml>
                </netcentric>
            </section>

            <wrapper
                cq:policy="netcentric/aemmjml/core/components/wrapper/default_policy"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/core/components/policies/mapping">
                <netcentric jcr:primaryType="nt:unstructured">
                    <aemmjml jcr:primaryType="nt:unstructured">
                        <core jcr:primaryType="nt:unstructured">
                            <components jcr:primaryType="nt:unstructured">
                                <column
                                    cq:policy="netcentric/aemmjml/core/components/column/default_policy"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="wcm/core/components/policies/mapping"/>
                                <group
                                    cq:policy="netcentric/aemmjml/core/components/group/default_policy"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="wcm/core/components/policies/mapping"/>
                                <hero
                                    cq:policy="netcentric/aemmjml/core/components/hero/default_policy"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="wcm/core/components/policies/mapping"/>
                                <section
                                    cq:policy="netcentric/aemmjml/core/components/section/default_policy"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="wcm/core/components/policies/mapping"/>
                            </components>
                        </core>
                    </aemmjml>
                </netcentric>

                <section
                    cq:policy="netcentric/aemmjml/core/components/section/default_policy"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/core/components/policies/mapping">
                    <netcentric jcr:primaryType="nt:unstructured">
                        <aemmjml jcr:primaryType="nt:unstructured">
                            <core jcr:primaryType="nt:unstructured">
                                <components jcr:primaryType="nt:unstructured">
                                    <group
                                        cq:policy="netcentric/aemmjml/core/components/group/default_policy"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="wcm/core/components/policies/mapping"/>
                                    <column
                                        cq:policy="netcentric/aemmjml/core/components/column/default_policy"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="wcm/core/components/policies/mapping"/>
                                </components>
                            </core>
                        </aemmjml>
                    </netcentric>
                </section>
            </wrapper>
        </root>
    </jcr:content>
</jcr:root>
